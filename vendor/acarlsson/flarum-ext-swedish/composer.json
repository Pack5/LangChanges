{
    "name": "acarlsson/flarum-ext-swedish",
    "type": "flarum-extension",
    "description": "Swedish language pack.",
    "keywords": ["locale", "language", "swedish", "svenska", "sv", "se"],
    "license": "MIT",
    "require": {
        "flarum/core": "^0.1.0-beta.5"
