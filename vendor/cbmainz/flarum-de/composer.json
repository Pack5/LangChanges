{
    "name": "cbmainz/flarum-de",
    "type": "flarum-extension",
    "description": "German language pack for Flarum and its bundled extensions.",
    "homepage": "https://flarum.today/de/",
    "keywords": ["locale"],
    "license": "MIT",
    "require": {
        "flarum/core": "^0.1.0-beta.6"
    },
    "extra": {
        "flarum-extension": {
            "title": "Deutsch",
            "icon": {
                "image": "icon.svg",
                "backgroundColor": "#00247d",
                "backgroundSize": "cover",
                "backgroundPosition": "center"
            }
        },
        "flarum-locale": {
            "code": "de",
            "title": "Deutsch"
        }
    }
}
